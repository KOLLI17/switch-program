import java.util.*;
class VowelConsonant
{
public static void main(String arg[])
{
Scanner sc= new Scanner(System.in);
char ch=sc.next().charAt(0);

switch(ch){
case 'a' :
case 'e' :
case 'i' :
case 'o' :
case 'u' :
System.out.println("is vowel");
break;
default:
System.out.println("is consonant");
break;
}
}
}
output:
a
 is vowel
 h
 is consonant
